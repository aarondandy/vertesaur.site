@using Vertesaur.Site
@inherits ServiceStack.Razor.ViewPage
@{
  ViewBag.Title = "All Namespaces";
  var codeDocRepositories = AppHost.TryResolve<CodeDocRepositories>();
}
<section>
  <h1>All Namespaces</h1>
  @if (codeDocRepositories == null) {
    <p>Code documentation is not available.</p>
  }
  else {
    Html.Partial("PartialCodeDocEntityList", codeDocRepositories.TargetRepository.Namespaces);
  }
</section>
